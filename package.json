{
  "name": "vue-directive-swipe-event",
  "version": "0.0.4",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly -p ./tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --max-warnings=0",
    "release": "standard-version",
    "prepare": "husky install",
    "test": "vitest"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "dependencies": {
    "vue": "^3.3.6",
    "vue-tsc": "^1.0.9"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.51.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.1",
    "prettier": "^2.7.1",
    "sass": "^1.69.4",
    "standard-version": "^9.5.0",
    "terser": "^5.22.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.2.2",
    "vite": "^3.2.1",
    "vitest": "^0.34.6"
  },
  "bugs": {
    "url": "https://github.com/theUkrainian/vue-swipe/issues"
  },
  "homepage": "https://github.com/theUkrainian/vue-swipe#readme",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theUkrainian/vue-swipe.git"
  }
}
